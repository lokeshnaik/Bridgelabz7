class Lokesh implements Runnable
{
	public void run()
	{

		int i;
		for(i=0;i<5;i++)
		{
			System.out.println("Hey Lokesh:");
			try
			   {
				   Thread.sleep(1000);
			   }
			   catch(Exception e)
			   {
				   
			   }
		}
		
	}		
}
class Naik implements Runnable
{
	public void run()
	{
		int i;
		for(i=0;i<5;i++)
		{
			System.out.println("Hardwork,willpower and dedication brngs u success");
			try
			   {
		         Thread.sleep(1000);	
			   }
			   catch(Exception e)
			   {
				   
			   }
	    }
		
	}		
}

public class firstclass
{
	public static void main(String [] args)
		
	{
		Lokesh loki=new Lokesh();
		Naik nk=new Naik();
		Thread t1=new Thread(loki);
		Thread t2=new Thread(nk);
		
		loki.run();
		try
		{
			Thread.sleep(100);
		}
		catch(Exception e)
		{
			
		}
		nk.run();
	}
}