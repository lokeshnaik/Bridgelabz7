public class firstclass
{
	public static void main(String[] args) throws Exception
	{
		
		
		
		Thread t1=new Thread(
				()->
				{
					for(int i=0;i<5;i++)
					{
						System.out.print("Hey Lokesh ");
						try
						{
							Thread.sleep(1000);
						}
						catch(Exception e)
						{
							
						}
					}
				}
				);
		Thread t2=new Thread(
				()->
				{
					for(int i=0;i<5;i++)
					{
						System.out.println("stay in limits");
						try
						{
							Thread.sleep(1000);
						}
						catch(Exception e)
						{
							
						}
					}
				});
		
		t1.start();
		System.out.println(t1.isAlive());
		try
		{
			Thread.sleep(10);
		}
		catch(Exception e)
		{
			
		}
		t2.start();
		t1.join();
		t2.join();
		System.out.println("Just crack the fellowship Lokesh");
	}
}