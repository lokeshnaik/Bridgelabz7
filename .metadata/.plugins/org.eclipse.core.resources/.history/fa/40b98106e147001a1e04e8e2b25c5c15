package com.bridgelabz.sampleprograms;

import java.util.Scanner;

class Stackprogram
{
	Node top=null;
	public void push(int data)
	{
		Node node=new Node();
		if(top==null)
		{
			System.out.println("Stack is full:");
		}
		else
		{
			 node.data=data;
			 top=top.next;
			 top=node;
		}
		
	}
	
	public void display()
	{
	   Node head=top;
	   while(head!=null)
	   {
		   System.out.print(head.data+"->");
		   head=head.next;
	   }
	   
	}
}

public class Stack {

	public static void main(String[] args)
	{
		Stackprogram stack=new Stackprogram();
	    int n;
	    Scanner sc=new Scanner(System.in);
	    n=sc.nextInt();
	    int [] array=new int[n];
	    for(int i=0;i<n;i++)
	    {
	    	array[i]=sc.nextInt();
	    }
	    for(int i=0;i<n;i++)
	    {
	       stack.push(array[i]);
	    }
	    stack.display();
		

	}

}
