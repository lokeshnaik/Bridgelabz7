abstract class Employee
{
	abstract void display();
	void display1()
	{
		System.out.println("Hey Lokesh:");
	}
	
}

class Person extends Employee
{
	void display()
	{
		System.out.println("Lokesh Naik");
	}
}

public class firstclass
{
	public static void main(String [] args)
	{
		Person p=new Person();
		p.display();
		p.display1();
	}
}