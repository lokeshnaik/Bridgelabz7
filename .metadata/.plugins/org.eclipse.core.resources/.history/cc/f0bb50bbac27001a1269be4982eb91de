
public class firstclass 
{
  public static void main(String[] args)
  {
	  int a=5,b=2;
	  int r1=a+b;
	  int r2=a-b;
	  int r3=a*b;
	  double r4=(double)a/b;
	  int r5=a%b;
	  System.out.println(r1);
	  System.out.println(r2);
	  System.out.println(r3);
	  System.out.println(r4);
	  System.out.println(r5);
  }
}
