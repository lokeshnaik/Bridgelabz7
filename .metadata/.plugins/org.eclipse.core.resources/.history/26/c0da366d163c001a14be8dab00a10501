class Lokesh
{
	public void show()
	{

		int i;
		for(i=0;i<5;i++)
		{
	   try
	   {
				System.out.print("Hey Lokesh ");
				Thread.sleep(1000);
			
	   }
	   catch(Exception e)
	   {
		   
	   }
		}
		
	}		
}
class Naik
{
	public void show()
	{
		int i;
		for(i=0;i<5;i++)
		{
	   try
	   {
			
				System.out.println(" Hardwork,willpower and dedication brngs u success");
				Thread.sleep(1000);
			
	   }
	   catch(Exception e)
	   {
		   
	   }
	}
		
	}		
}

public class firstclass
{
	public static void main(String [] args)
		
	{
		Lokesh loki=new Lokesh();
		Naik nk=new Naik();
		
		loki.show();
		try
		{
			Thread.sleep(100);
		}
		catch(Exception e)
		{
			
		}
		nk.show();
	}
}