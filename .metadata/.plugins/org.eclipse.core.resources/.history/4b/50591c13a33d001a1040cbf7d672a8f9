		int val1=9;
		String [] expectedArray= {"Lokesh","Loki","Naik"};
		String [] resultArray= {"Lokesh","Loki","Naik"};
		
		assertEquals(str1,str2);
		//Check for Equal
		assertTrue(val<val1);
		//Check for True
		assertTrue(val<val1);
		//check for False
		assertNull( str3);
		//Check for Null
		
	}

