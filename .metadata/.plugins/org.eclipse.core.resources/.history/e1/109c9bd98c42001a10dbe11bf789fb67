import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

import com.bridgelabz.utility.Utility;

class StringReplace {

	@Test
	void test() {
	String string=Utility.stringReplace("Hello <<UserName>>,How are you?", "LOKESH NAIK");
	assertEquals("Hello LOKESH NAIK,How are you?",string);
	
	}
	@Test
	void test1() {
	String string=Utility.stringReplace("Hello <<UserName>>,How are you?", "SUMIT");
	assertEquals("Hello SUMIT,How are you?",string);
	
	}
	@Test
	void test2() {
	String string=Utility.stringReplace("Hello <<UserName>>,How are you?", "SACHIN");
	assertEquals("Hello SACHIN,How are you?",string);
	
	}

}
