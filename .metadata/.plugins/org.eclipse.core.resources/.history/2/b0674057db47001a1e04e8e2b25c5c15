package com.bridgelabz.sampleprograms;

public class LinkedList
{
     Node head;
   public void insert(String data)
   {
	   Node node=new Node();
	   node.data=data;
	   node.next=null;
	   if(head==null)
	   {
		   head=node;
	   }
	   else
	   {
		   Node n=head;
		   while(n.next!=null)
		   {
			   n=n.next;
		   }
		   n.next=node;
	   }
	   
   }
   
   public void show()
   {
	   Node p=head;
	   while(p!=null)
	   {
		   System.out.print(p.data+"->");
		   p=p.next;
	   }
   }
   
}
