public class firstclass
{
	public static void main(String [] args)throws Exception
	{
		Runnable obj=new Runnable()
				{
			      public void run()
			      {
			    	  int i;
			    		 for(i=0;i<5;i++)
			    		 {
			    	 try
			    	 {
			    		 
			    			 System.out.println("Hey Lokesh ");
			    			 Thread.sleep(1000);
			    		 
			    	 }
			    	 catch(Exception e)
			    	 {
			    		  
			    	 }
			      }
			      }
				};
				Runnable obj1=new Runnable()
						{
					       public void run()
					       {
					    	   int i;
					    	   for(i=0;i<5;i++)
					    	   {
					    		 
					    	  try
					    	  {
					    		    System.out.println("Hardwork,willpower and dedication");
						    		   Thread.sleep(1000);
						    	   
					    	  }
					    	  catch(Exception e)
					    	  {
					    		  
					    	  }
					       }
					       }
						};
						Thread t1=new Thread(obj);
						Thread t2=new Thread(obj1);
						
						t1.start();
						try
						{
							Thread.sleep(100);
						}
						catch(Exception e)
						{
							
						}
						t2.start();
						System.out.println(t1.isAlive());
						t1.join();
						
						
						t2.join();
						
						System.out.println(t2.isAlive());
						System.out.println("Hey just do it bro:");
	         }
}