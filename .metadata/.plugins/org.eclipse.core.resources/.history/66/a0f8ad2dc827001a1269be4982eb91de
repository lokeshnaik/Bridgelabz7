
class operation
{
	int num1;
	int num2;
	int result;
	public void perform()
	{
		result=num1+num2;
	}
}



public class firstclass
{
	public static void main(String[] args)
	{
	   operation op=new operation();
	   op.num1=4;
	   op.num2=5;
	   
	   op.perform();
	   System.out.print(op.result);
	}
}
