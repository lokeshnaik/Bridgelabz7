public class firstclass
{
	public static void main(String [] args) throws Exception
	{
		Thread obj1=new Thread(()->
		{
			int i;
			for(i=0;i<5;i++)
			{
				System.out.println("Hey Lokesh ");
				try 
				{
					Thread.sleep(1000);
				}
				catch(Exception e)
				{
					
				}
			}
		},"Lokesh Thread");
		Thread obj2=new Thread(()->
		{
			int i;
			for(i=0;i<5;i++)
			{
				System.out.println("stay in your limits");
				try 
				{
					Thread.sleep(1000);
				}
				catch(Exception e)
				{
					
				}
			}
		},"Naik Thread");
		
		
		System.out.println(obj1.getName());
		System.out.println(obj2.getName());
		
		obj1.start();
		try
		{
			Thread.sleep(10);
		}
		catch(Exception e)
		{
			
		}
		
		obj2.start();
		System.out.println(obj2.isAlive());
		
		obj1.join();
		obj2.join();
		
		System.out.println("Just crack the fellowship Lokesh");
		
	}
}